<?xml version="1.0" encoding="utf-8"?>
<!-- 월간 캘린더에서 날짜 선택 on/off에 따른 이벤트 처리 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 날짜 선택했을 때 -->
    <item android:state_checked="true" android:drawable="@drawable/ic_monthly_event_day"/>
    <item android:state_pressed="true" android:drawable="@drawable/ic_monthly_event_day"/>

    <!-- 날짜 선택 안 했을 때 -->
    <item android:drawable="@color/transparent"/>
</selector>